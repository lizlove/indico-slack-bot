{
  "name": "indico-slack-bot",
  "version": "1.0.1",
  "description": "Bot that sends daily updates of Indico events to Slack.",
  "main": "app.js",
  "keywords": [
    "slack",
    "bot",
    "indico",
    "slash-commands",
    "slack-app"
  ],
  "scripts": {
    "lint": "tsc && eslint \"**/*.{js,ts}\" --fix",
    "lint:quiet": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "tsc && node dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts"
  },
  "contributors": [{
    "name": "Elizabeth Lovero",
    "url": "https://github.com/lizlove"
  }],
  "dependencies": {
    "@slack/bolt": "^2.3.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "gts": "^2.0.2",
    "nodemon": "^2.0.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.8.3"
  },
  "homepage": "https://github.com/lizlove/indico-slack-bot#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizlove/indico-slack-bot.git"
  },
  "bugs": {
    "url": "https://github.com/lizlove/indico-slack-bot/issues"
  }
}